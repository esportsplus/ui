{
    "author": "ICJR",
    "dependencies": {
        "@esportsplus/action": "^0.0.58",
        "@esportsplus/queue": "^0.1.0",
        "@esportsplus/reactivity": "^0.18.1",
        "@esportsplus/template": "^0.26.5",
        "@esportsplus/utilities": "^0.25.0",
        "@esportsplus/vite": "^0.10.5",
        "modern-normalize": "^3.0.1"
    },
    "devDependencies": {
        "@esportsplus/typescript": "^0.9.2",
        "@types/node": "^24.10.1",
        "@types/shelljs": "^0.8.17",
        "autoprefixer": "^10.4.22",
        "glob": "^13.0.0",
        "lightningcss": "^1.30.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.94.2",
        "shelljs": "^0.10.0",
        "vite": "^7.2.2"
    },
    "exports": {
        "./css-utilities.scss": "./build/css-utilities/index.scss",
        "./css-utilities/*.scss": "./build/css-utilities/*/scss/index.scss",
        "./fonts": {
            "types": "./build/fonts/index.d.ts",
            "default": "./build/fonts/index.js"
        },
        "./layer.scss": "./layer.scss",
        "./lib.scss": "./lib.scss",
        "./themes/dark": {
            "types": "./build/themes/dark/index.d.ts",
            "default": "./build/themes/dark/index.js"
        },
        "./themes/dark/*.scss": "./build/themes/dark/*/scss/index.scss",
        "./themes/light": {
            "types": "./build/themes/light/index.d.ts",
            "default": "./build/themes/light/index.js"
        },
        "./themes/light/*.scss": "./build/themes/light/*/scss/index.scss",
        "./tokens.scss": "./tokens.scss",
        "./*.scss": "./build/components/*/scss/index.scss",
        "./*": {
            "types": "./build/components/*/index.d.ts",
            "default": "./build/components/*/index.js"
        },
        ".": {
            "types": "./build/components/index.d.ts",
            "default": "./build/components/index.js"
        }
    },
    "name": "@esportsplus/ui",
    "private": false,
    "scripts": {
        "build": "run-s build:vite build:ts",
        "build:ts": "tsc && tsc-alias",
        "build:vite": "vite build",
        "-": "-",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm build"
    },
    "sideEffects": false,
    "type": "module",
    "version": "0.45.5"
}
