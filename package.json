{
    "author": "ICJR",
    "dependencies": {
        "@esportsplus/action": "^0.0.58",
        "@esportsplus/queue": "^0.1.0",
        "@esportsplus/reactivity": "^0.16.7",
        "@esportsplus/template": "^0.22.5",
        "@esportsplus/utilities": "^0.22.1",
        "modern-normalize": "^3.0.1"
    },
    "devDependencies": {
        "@esportsplus/typescript": "^0.9.2",
        "@types/node": "^24.3.0",
        "autoprefixer": "^10.4.21",
        "glob": "^11.0.3",
        "lightningcss": "^1.30.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.90.0",
        "vite": "^7.1.3"
    },
    "exports": {
        "./css-utilities": {
            "types": "./build/css-utilities/index.d.ts",
            "default": "./build/css-utilities/index.js"
        },
        "./css-utilities/*": "./build/css-utilities/*/scss/index.scss",
        "./fonts": {
            "types": "./build/fonts/index.d.ts",
            "default": "./build/fonts/index.js"
        },
        "./layer": "./layer.scss",
        "./lib": "./lib.scss",
        "./normalize": {
            "types": "./build/normalize/index.d.ts",
            "default": "./build/normalize/index.js"
        },
        "./tokens": "./tokens.scss",
        "./*": {
            "types": "./build/components/*/index.d.ts",
            "default": "./build/components/*/index.js"
        },
        ".": {
            "types": "./build/components/index.d.ts",
            "default": "./build/components/index.js"
        }
    },
    "name": "@esportsplus/ui",
    "private": false,
    "scripts": {
        "build": "run-s build:vite build:ts",
        "build:ts": "tsc && tsc-alias",
        "build:vite": "vite build",
        "-": "-",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm build"
    },
    "sideEffects": false,
    "type": "module",
    "version": "0.21.0"
}
